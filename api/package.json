{
  "name": "gerir-me-api",
  "version": "1.0.0",
  "description": "API REST para o sistema Gerir.me - Gerenciamento de Despesas Pessoais",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "mocha test/**/*.test.js --reporter mochawesome --reporter-options reportDir=test-results,reportFilename=api-test-report",
    "test:watch": "mocha test/**/*.test.js --watch",
    "test:coverage": "nyc mocha test/**/*.test.js"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "nodejs",
    "expense-management",
    "portfolio",
    "testing"
  ],
  "author": "Edcleryton",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
